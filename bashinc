make() { if [ $1 = "al" ]; then 
	cat ~/.extras/al;
else
	make $@;
fi
}

function dig {
url=`echo $1 | sed 's/https\?:\/\///'`;
url=`echo $url | sed 's/\/$//'`;
ip=`command dig $url +short`;
reverseip=`command dig -x $ip +short`;
echo $ip;
echo $reverseip;
}

function mykey() {
	cat ~/.ssh/id_rsa.pub
}