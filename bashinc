make() { if [ $1 = "al" ]; then 
	cat ~/.extras/al;
else
	make $@;
fi
}

function dig {
url=`echo $1 | sed 's/https\?:\/\///'`;
url=`echo $url | sed 's/\/$//'`;
ip=`command dig $url +short`;
reverseip=`command dig -x $ip +short`;
nameservers=`whois $url | grep 'Name Server' | sed 's/Name\ Server\://' | sed 's/\ //g'`
echo $ip;
echo $reverseip;
echo $nameservers;
}
